<Tela>:
    orientation:'vertical'
    canvas.before:
        Rectangle:
            pos:self.pos
            size:self.size
            source:'fundoFeed.jpg'
    Label:
        id:nome
        text:root.get_name()
        size_hint:[.2,.1]
        pos_hint:{'x':.4}
        font_size:50
        color:[0,0,0,1]
    Label:
        id:historia
        text:root.hist_plus_rules()
        size_hint:[.5,.5]
        pos_hint:{'x':.25,'y':.7}
        text_size: (930,None)
        color:[0,0,0,1]
