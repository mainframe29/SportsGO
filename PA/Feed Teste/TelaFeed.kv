<Feed>:
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'fundoFeed.jpg'

    canvas:
        Ellipse:
            id: icon
            pos: self.center_x * 1.87, self.center_y * 1.82 
            size: 50,50
            source: 'images.png'
